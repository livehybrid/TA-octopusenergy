{
  "pages": {
    "configuration": {
      "tabs": [
        {
          "name": "proxy",
          "entity": [
            {
              "type": "checkbox",
              "label": "Enable",
              "field": "proxy_enabled"
            },
            {
              "type": "singleSelect",
              "label": "Proxy Type",
              "options": {
                "disableSearch": true,
                "autoCompleteFields": [
                  {
                    "value": "http",
                    "label": "http"
                  },
                  {
                    "value": "socks4",
                    "label": "socks4"
                  },
                  {
                    "value": "socks5",
                    "label": "socks5"
                  }
                ]
              },
              "defaultValue": "http",
              "field": "proxy_type"
            },
            {
              "type": "text",
              "label": "Host",
              "validators": [
                {
                  "type": "regex",
                  "errorMsg": "Proxy Host should not have special characters",
                  "pattern": "^[a-zA-Z]\\w*$"
                },
                {
                  "type": "string",
                  "errorMsg": "Max host length is 4096",
                  "minLength": 0,
                  "maxLength": 4096
                }
              ],
              "field": "proxy_url"
            },
            {
              "type": "text",
              "label": "Port",
              "validators": [
                {
                  "type": "number",
                  "range": [1, 65535]
                }
              ],
              "field": "proxy_port"
            },
            {
              "type": "text",
              "label": "Username",
              "validators": [
                {
                  "type": "string",
                  "errorMsg": "Max length of username is 50",
                  "minLength": 0,
                  "maxLength": 50
                }
              ],
              "field": "proxy_username"
            },
            {
              "type": "text",
              "label": "Password",
              "validators": [
                {
                  "type": "string",
                  "errorMsg": "Max length of password is 8192",
                  "minLength": 0,
                  "maxLength": 8192
                }
              ],
              "encrypted": true,
              "field": "proxy_password"
            },
            {
              "type": "checkbox",
              "label": "Reverse DNS resolution",
              "field": "proxy_rdns"
            }
          ],
          "options": {
            "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } if(!formData.proxy_type) {return 'Proxy type can not be empty'; } return true; }"
          },
          "title": "Proxy"
        },
        {
          "name": "logging",
          "entity": [
            {
              "type": "singleSelect",
              "label": "Log level",
              "options": {
                "disableSearch": true,
                "autoCompleteFields": [
                  {
                    "value": "DEBUG",
                    "label": "DEBUG"
                  },
                  {
                    "value": "INFO",
                    "label": "INFO"
                  },
                  {
                    "value": "WARNING",
                    "label": "WARNING"
                  },
                  {
                    "value": "ERROR",
                    "label": "ERROR"
                  },
                  {
                    "value": "CRITICAL",
                    "label": "CRITICAL"
                  }
                ]
              },
              "defaultValue": "INFO",
              "field": "loglevel"
            }
          ],
          "title": "Logging"
        },
        {
          "name": "account",
          "table": {
            "actions": ["edit", "delete", "clone"],
            "header": [
              {
                "label": "Name",
                "field": "name"
              }
            ]
          },
          "entity": [
            {
              "type": "text",
              "label": "OctopusEnergy account username",
              "required": false,
              "validators": [
                {
                  "type": "string",
                  "errorMsg": "Length of ID should be between 1 and 50",
                  "minLength": 1,
                  "maxLength": 50
                },
                {
                  "type": "regex",
                  "errorMsg": "Name must begin with a letter and consist exclusively of alphanumeric characters, hyphens, fullstops, @ symbol and underscores.",
                  "pattern": "^[a-zA-Z][\\.\\@\\-\\w]+$"
                }
              ],
              "options": {
                "placeholder": "you@youremail.com"
              },
              "defaultValue": "",
              "field": "name",
              "encrypted": false
            },
            {
              "type": "text",
              "label": "OctopusEnergy account password",
              "required": false,
              "options": {
                "placeholder": "*********"
              },
              "defaultValue": "",
              "field": "account_password",
              "encrypted": true
            },
            {
              "type": "text",
              "label": "Account Number",
              "required": false,
              "options": {
                "placeholder": "Leave Blank - Autopopulated on save"
              },
              "field": "account_number"
            },
            {
              "type": "text",
              "label": "Refresh Token",
              "required": false,
              "options": {
                "placeholder": "Leave Blank - Autopopulated on save"
              },
              "defaultValue": "",
              "field": "refresh_token",
              "encrypted": true
            },
            {
              "type": "text",
              "label": "Access Token",
              "required": false,
              "options": {
                "placeholder": "Leave Blank - Autopopulated on save"
              },
              "defaultValue": "",
              "field": "access_token",
              "encrypted": true
            }
          ],
          "title": "Accounts"
        }
      ],
      "title": "Configuration",
      "description": "Configure the Add-on"
    },
